<%= form_for @profile, html: {multipart: true} do |f| %>
  <p>
  	<%= f.label :first_name  %> <br>
    <%= f.text_field :first_name, autofocus: true %>
  </p>
  <p>
  	<%= f.label :last_name  %> <br>
    <%= f.text_field :last_name, autofocus: true %>
  </p>
    <p>
  	<%= f.label :contact_email  %> <br>
    <%= f.text_field :contact_email %>
  </p>
  <p>
    <%= f.label "Facebook account"  %> <br>
    <%= f.text_field :facebook_url %>
  </p>
   <p>
    <%= f.label "Twitter account"  %> <br>
    <%= f.text_field :twitter_url %>
  </p>
   <p>
    <%= f.label "Instagram account"  %> <br>
    <%= f.text_field :instagram_url %>
  </p>
   <p>
    <%= f.label "Tumblr account"  %> <br>
    <%= f.text_field :tumblr_url %>
  </p>
   <p>
    <%= f.label "Pinterest account"  %> <br>
    <%= f.text_field :pinterest_url %>
  </p>
  <p>
    <%= f.label "Linkedin account"  %> <br>
    <%= f.text_field :linkedin_url %>
  </p>
   <p>
    <%= f.label "Soundcloud account"  %> <br>
    <%= f.text_field :soundcloud_url %>
  </p>
     <p>
    <%= f.label "YouTube account"  %> <br>
    <%= f.text_field :youtube_url %>
  </p>
  <p>
    <%= f.label "Personal Info"  %> <br>
    <%= f.text_area :bio %>
  </p>


  <%= f.submit  %>
  
<% end %>